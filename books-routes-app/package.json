{
  "name": "books-routes-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://booksroutes.info/",
  "dependencies": {
    "@material-ui/core": "^1.3.0",
    "@material-ui/icons": "^1.1.0",
    "flow-bin": "^0.75.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "react": "^16.3.0",
    "react-dom": "^16.2.0",
    "react-ga": "^2.4.1",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^5.2.0",
    "react-progressive-image": "^0.3.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-snap": "https://github.com/valkurash/react-snap/tarball/add-sitemap",
    "redux": "^4.0.0",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prebuild": "node imagemin.js",
    "postbuild":
      "react-snap && npm run generate-sw && node iisConfig/copyIISConfig.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "generate-sw":
      "sw-precache --root=build --config sw-precache-config.js && uglifyjs build/service-worker.js -o build/service-worker.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.0.1",
    "eslint-config-google": "^0.9.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^3.0.1",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-mozjpeg": "^7.0.0",
    "imagemin-pngquant": "^5.1.0",
    "prettier-eslint": "^8.8.1",
    "redux-devtools": "^3.4.1"
  },
  "reactSnap": {
    "sitemap": true
  }
}
