{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "worker": true,
    "jest": true
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:import/typescript"],
  "rules": {
    "no-console": 0
  }
}
